class oneBHK {
	int roomArea;
	int hallArea;
	static int price;
	
	oneBHK(){
		this.roomArea = 15;
		this.hallArea = 20;
		
		oneBHK.price = 2000;
	}
	oneBHK(int roomArea,int hallArea,int price){
		this.hallArea = hallArea;
		this.roomArea = roomArea;
		price = price;
	}
}
