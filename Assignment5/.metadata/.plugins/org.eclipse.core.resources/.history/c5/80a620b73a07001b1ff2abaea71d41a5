abstract class Processor{
	int data;
 	void showData() {
		System.out.println(data);
	}
	abstract void process(int no);
}
class Factorial extends Processor{
	int result = 1;
	void process (int no1) {
		for(int i= no1 ;i>0; i--)
		{
			result=result*i;
		}
		
		data=result;
	}
	
}

public class que48 {

	public static void main(String[] args) {
 
	}

}
