class Student {
	int rollno;
	float percentage;
	
	public Student() {
	
	}
	Student(int roll,float per){
		this.percentage = per;
		this.rollno = roll;
	}
	void show() {
		System.out.println("Roll No :"+rollno);
		System.out.println("Percentage : "+percentage);
	}
}

